{
  "name": "axioned-starter",
  "version": "1.1.0",
  "description": "A scaffold for developers to get started with projects at Axioned. This will be expanded to create starter repos for React, Wordpress and Node projects as well.",
  "main": "index.js",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.7",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^27.4.6",
    "babel-polyfill": "^6.26.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.20",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.2.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --config jest.config.js",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "post-merge": "./.husky/post-merge"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prdxn-org/axioned-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prdxn-org/axioned-starter/issues"
  },
  "homepage": "https://github.com/prdxn-org/axioned-starter#readme",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "post-merge": "./.githooks/post-merge"
    }
  },
  "engines": {
    "npm": ">=7.10.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@babel/preset-react": "^7.16.7",
    "next": "^13.0.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
